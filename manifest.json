{
  "name": "Modisfill",
  "version": "1.0",
  "manifest_version": 2,
  "description": "",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "https://candidate.it.modis.com/",
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://candidate.it.modis.com/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
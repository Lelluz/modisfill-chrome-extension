{
  "name": "Modisfill",
  "version": "1.0",
  "manifest_version": 2,
  "description": "",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "<all_urls>",
    "activeTab",
    "storage",
    "notifications"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}